{
  "name": "webpieces-ts-example",
  "version": "1.0.0",
  "description": "Example application using WebPieces TypeScript framework",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "use-local-webpieces": "./scripts/use-local-webpieces.sh",
    "use-local-webpieces-link": "./scripts/use-local-webpieces-link.sh",
    "unlink-local-webpieces": "./scripts/unlink-local-webpieces.sh",
    "use-published-webpieces": "./scripts/use-published-webpieces.sh",
    "build-webpieces": "./scripts/build-webpieces.sh",
    "debug:wait": "node --inspect-brk dist/services/website/server/main.js",
    "debug:serve": "npx nx serve server --configuration=development",
    "build-all": "npm run build-all:compile && npm run build-all:lint && npm run build-all:test && npm run build-all:architecture",
    "build-all:compile": "npx nx run-many --target=build --all",
    "build-all:lint": "npx nx run-many --target=lint --all",
    "build-all:test": "npx nx run-many --target=test --all",
    "build-all:architecture": "npx nx run architecture:validate-no-cycles && npx nx run architecture:validate-no-skiplevel-deps && npx nx run architecture:validate-architecture-unchanged",
    "arch:generate": "nx run .:arch:generate",
    "arch:visualize": "nx run .:arch:visualize",
    "arch:validate": "nx run .:arch:validate-no-cycles && nx run .:arch:validate-no-skiplevel-deps",
    "arch:validate-all": "nx run .:arch:validate-no-cycles && nx run .:arch:validate-no-skiplevel-deps && nx run .:arch:validate-architecture-unchanged",
    "arch:check-circular": "nx run-many --target=check-circular-deps --all",
    "arch:check-circular-affected": "nx affected --target=check-circular-deps",
    "arch:validate-complete": "npm run arch:validate-all && npm run arch:check-circular"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deanhiller/webpieces-ts-example.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/deanhiller/webpieces-ts-example/issues"
  },
  "homepage": "https://github.com/deanhiller/webpieces-ts-example#readme",
  "devDependencies": {
    "@angular-devkit/core": "~20.3.0",
    "@angular-devkit/schematics": "~20.3.0",
    "@angular/build": "~20.3.0",
    "@angular/cli": "~20.3.0",
    "@angular/compiler-cli": "~20.3.0",
    "@angular/language-service": "~20.3.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "^22.1.3",
    "@nx/devkit": "22.1.3",
    "@nx/esbuild": "22.1.3",
    "@nx/eslint": "22.1.3",
    "@nx/eslint-plugin": "22.1.3",
    "@nx/jest": "22.1.3",
    "@nx/js": "^22.1.3",
    "@nx/node": "^22.1.3",
    "@nx/playwright": "22.1.3",
    "@nx/web": "22.1.3",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "~20.3.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "^1.15.3",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "20.19.9",
    "@typescript-eslint/utils": "^8.40.0",
    "@webpieces/dev-config": "^0.2.29",
    "angular-eslint": "^20.3.0",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-playwright": "^1.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "~14.6.1",
    "jest-util": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "madge": "^8.0.0",
    "nx": "^22.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.40.0"
  },
  "dependencies": {
    "@angular/common": "~20.3.0",
    "@angular/compiler": "~20.3.0",
    "@angular/core": "~20.3.0",
    "@angular/forms": "~20.3.0",
    "@angular/platform-browser": "~20.3.0",
    "@angular/platform-browser-dynamic": "~20.3.0",
    "@angular/router": "~20.3.0",
    "@inversifyjs/binding-decorators": "^1.1.5",
    "@webpieces/client": "^0.2.29",
    "@webpieces/rules": "^0.2.29",
    "@webpieces/server": "^0.2.29",
    "express": "^4.21.2",
